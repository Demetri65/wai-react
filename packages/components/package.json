{
  "name": "@wai-react/components",
  "version": "0.0.1",
  "sideEffects": false,
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./src/index.tsx",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./heading": {
      "types": "./src/heading/heading.tsx",
      "import": "./dist/heading/heading.mjs",
      "require": "./dist/heading/heading.js"
    },
    "./list": {
      "types": "./src/list/list.tsx",
      "import": "./dist/list/list.mjs",
      "require": "./dist/list/list.js"
    },
    "./text": {
      "types": "./src/text/text.tsx",
      "import": "./dist/text/text.mjs",
      "require": "./dist/text/text.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint . --max-warnings 0",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "devDependencies": {
    "@wai-react/eslint-config": "workspace:*",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "eslint": "^8.57.0",
    "@wai-react/typescript-config": "workspace:*",
    "tsup": "^8.0.2",
    "typescript": "5.5.4"
  },
  "dependencies": {
    "react": "^18.2.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
