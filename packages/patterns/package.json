{
  "name": "@wai-react/patterns",
  "version": "0.0.1",
  "sideEffects": false,
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./src/index.tsx",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./accordion": {
      "types": "./src/accordion/accordion.tsx",
      "import": "./dist/accordion/accordion.mjs",
      "require": "./dist/accordion/accordion.js"
    },
    "./landmarks": {
      "types": "./src/landmarks/index.tsx",
      "import": "./dist/landmarks/index.mjs",
      "require": "./dist/landmarks/index.js"
    },
    "./landmarks/complimentary": {
      "types": "./src/landmarks/complimentary/complimentary.tsx",
      "import": "./dist/landmarks/complimentary/complimentary.mjs",
      "require": "./dist/landmarks/complimentary/complimentary.js"
    },
    "./landmarks/region": {
      "types": "./src/landmarks/region/region.tsx",
      "import": "./dist/landmarks/region/region.mjs",
      "require": "./dist/landmarks/region/region.js"
    },
    "./list-box": {
      "types": "./src/list-box/list-box.tsx",
      "import": "./dist/list-box/list-box.mjs",
      "require": "./dist/list-box/list-box.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint . --max-warnings 0",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "devDependencies": {
    "@wai-react/eslint-config": "workspace:*",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "eslint": "^8.57.0",
    "@wai-react/typescript-config": "workspace:*",
    "tsup": "^8.0.2",
    "typescript": "5.5.4"
  },
  "dependencies": {
    "@wai-react/components": "workspace:*",
    "react": "^18.2.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
